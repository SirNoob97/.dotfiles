{
    "workbench.colorTheme": "Default Dark+",
    "workbench.enableExperiments": false,
    "workbench.editor.enablePreview": false,
    "[java]": {
        "editor.defaultFormatter": "mwpb.java-prettier-formatter",
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/node_modules": true
    },
    "editor.tabSize": 2,
    "editor.fontSize": 12,
    "editor.fontFamily": "'FiraCode Nerd Font Mono'",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingDelete": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.trimAutoWhitespace": false,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": false,
    "editor.wordWrap": "off",
    "editor.rename.enablePreview": false,
    "window.autoDetectHighContrast": false,
    "window.menuBarVisibility": "toggle",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.localEchoExcludePrograms": [],
    "notebook.compactView": false,
    "notebook.diff.enablePreview": false,
    "notebook.dragAndDropEnabled": false,
    "notebook.breadcrumbs.showCodeCells": false,
    "update.mode": "none",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/home/martin/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
    "advancedNewFile.exclude": {
        "node_modules": true,
        "node_modules_electron": true,
        "dev": true,
        "dist": true
    },
    "advancedNewFile.showInformationMessages": true,
    "advancedNewFile.convenienceOptions": [
        "last",
        "current",
        "root"
    ],
    "telemetry.enableCrashReporter": false,
    "git.autoRepositoryDetection": "openEditors",
    "git.decorations.enabled": false,
    "git.autorefresh": false,
    "scm.diffDecorations": "none",
    "breadcrumbs.enabled": false,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "g",
                "d"
            ],
            "commands": [
                "editor.action.revealDefinition"
            ]
        },
        {
            "before": [
                "g",
                "D"
            ],
            "commands": [
                "editor.action.revealDeclaration"
            ]
        },
        {
            "before": [
                "g",
                "i"
            ],
            "commands": [
                "editor.action.goToImplementation"
            ]
        },
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "g",
                "t"
            ],
            "commands": [
                "editor.action.goToTypeDefinition"
            ]
        },
        {
            "before": [
                "c",
                "a"
            ],
            "commands": [
                "editor.action.sourceAction"
            ]
        },
        {
            "before": [
                "c",
                "d"
            ],
            "commands": [
                "workbench.action.problems.focus"
            ]
        },
        {
            "before": [
                "]",
                "]"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "[",
                "["
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "editor.action.organizeImports"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<TAB>"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "<S-TAB>"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "<S-k>"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "/",
                "/"
            ],
            "commands": [
                "editor.action.addCommentLine"
            ]
        },
        {
            "before": [
                "\\",
                "\\"
            ],
            "commands": [
                "editor.action.blockComment"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "workbench.action.focusSideBar"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                "workbench.files.action.focusOpenEditorsView"
            ]
        },
        {
            "before": [
                "+",
                "+"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        {
            "before": [
                "-",
                "-"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "<CR>"
            ],
            "commands": [
                ":nohl<CR><CR>"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "+",
                "+"
            ],
            "commands": [
                "editor.action.moveLinesUpAction"
            ]
        },
        {
            "before": [
                "-",
                "-"
            ],
            "commands": [
                "editor.action.moveLinesDownAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "editor.action.formatSelection"
            ]
        }
    ],
    "editor.codeLens": false,
    "editor.detectIndentation": false,
    "editor.hover.enabled": false,
    "editor.lineNumbers": "relative",
    "editor.renderIndentGuides": false,
    "editor.cursorBlinking": "solid",
    "editor.inlineSuggest.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.tips.enabled": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "search.showLineNumbers": true,
    "terminal.integrated.customGlyphs": true,
    "task.autoDetect": "off",
    "update.showReleaseNotes": false,
    "telemetry.enableTelemetry": false,
    "java.autobuild.enabled": true,
    "github.gitAuthentication": false,
    "workbench.editor.restoreViewState": false,
    "files.restoreUndoStack": false,
    "workbench.startupEditor": "none",
    "editor.accessibilitySupport": "off",
}